package Lec5_Loops;

import java.util.*;

public class Exercise3 {

    public static void main(String[] args) {
        Random random = new Random();
        int n = random.nextInt(80 - 3 + 1) + 3;
        System.out.println("So nguyen la: " + n);
        if (n % 2 == 0) {
            System.out.print("So nguyen la so chan ");
        } else {
            System.out.print("So nguyen la so le ");
        }

    }
}
